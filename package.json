    "react": "18.1.0",
    "react-native": "0.70.6",
    "react-redux": "^8.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/jest-native": "^5.3.3",
    "@testing-library/react-native": "^11.5.0",
    "babel-jest": "^26.6.3",
    "cross-fetch": "^3.1.5",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.72.3",
    "react-test-renderer": "18.1.0"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/__mocks__/setup.js"
